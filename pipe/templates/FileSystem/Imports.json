    "name" :  "Import CSV",
    "args" : [ { "name" : "filepath" } ],
    "outs" : [ { "name" : "output" } ],
    "code" : [
      "f = open(filepath, 'r')",
      "lines = f.readlines()",
      "f.close()",
      "",
      "# Setup map",
      "output = {}",
      "keys = [p.strip() for p in lines[0].split(',')]",
      "for key in keys:",
      "    output[key] = []",
      "",
      "# Add data",
      "for line in lines[1:]:",
      "    if not line.strip(): continue",
      "    parts = line.strip().split(',')",
      "    for (ix, p) in enumerate(parts):",
      "        value = float(p)",
      "        output[keys[ix]].append(value)",
      ""
    ]
  }
